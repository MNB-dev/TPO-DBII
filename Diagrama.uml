
@startuml

class Hotel {
    + nombre: String
    + dirección: String
    + teléfono: String
    + email: String
}

class Habitación {
    + número: String
    + tipo: String
    + amenities: List<String>
    + precio: Float
}

class Amenity {
    + nombre: String
    + descripción: String
}

class Punto_de_Interés {
    + nombre: String
    + descripción: String
    + tipo: String
    + distancia: Float
}

class Huésped {
    + nombre: String
    + apellido: String
    + teléfono: String
    + email: String
    + dirección: String
}

class Reserva {
    + código_de_reserva: String
    + fecha_de_inicio: Date
    + fecha_de_salida: Date
    + tarifa: Float
}

Hotel "1" -- "*" Habitación : tiene
Hotel "1" -- "*" Punto_de_Interés : tiene
Habitación "1" -- "*" Amenity : tiene
Huésped "1" -- "*" Reserva : tiene
Reserva "1" -- "1" Habitación : está asociada a

@enduml
